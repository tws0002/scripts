<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.adobe.htmlsample.tactic" ExtensionBundleVersion="1.0" Version="5.0">
    <ExtensionList>
        <Extension Id="com.adobe.htmlsample.tactic.panel" Version="1.0"/>
